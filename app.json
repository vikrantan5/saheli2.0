{
  "expo": {
    "name": "Saheli",
    "slug": "saheli",
    "scheme": "saheli",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Saheli needs your location to provide safety features and emergency assistance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Saheli needs your location in the background to continuously monitor your safety and provide emergency assistance.",
        "NSLocationAlwaysUsageDescription": "Saheli needs your location in the background to keep you safe and help in emergencies."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDKz4KM0oHphvPQtM7DhG9QfpaHp4ql4bM"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "xyz.create.CreateExpoEnvironment",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDKz4KM0oHphvPQtM7DhG9QfpaHp4ql4bM"
        }
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "sitemap": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-audio",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-video",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Saheli to use your location for safety monitoring and emergency assistance.",
          "locationAlwaysPermission": "Allow Saheli to use your location in the background to keep you safe.",
          "locationWhenInUsePermission": "Allow Saheli to use your location for safety features.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
